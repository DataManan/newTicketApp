{% extends "bootstrap/base.html" %} {% import "bootstrap/wtf.html" as wtf %} {%
block title %} Create Venue {% endblock %} {% block styles %}
{{ super() }}
<link rel="stylesheet" href="../../static/styles/create_venue.css" />
{% endblock %} {% block content %}
<div class="container m-5">
  <div class="row">
    <div class="col-3">
      <form class="form-signin" method="POST">
        <h2 class="form-signin-heading">Edit Venue</h2>
        {{ form.hidden_tag() }}

        {{ wtf.form_field(form.venue_name) }}

        {{ wtf.form_field(form.capacity) }}

        {{ wtf.form_field(form.street) }}
        {{ wtf.form_field(form.city) }}
        {{ wtf.form_field(form.state) }}

        {{ wtf.form_field(form.venue_tags) }}

        <button class="btn btn-lg btn-primary btn-block" type="submit">
          Submit
        </button>
      </form>
    </div>
  </div>
</div>
{% endblock content %}
